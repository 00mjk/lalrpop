use generics_issue_104_lib::Generator;

grammar<T> where T: Generator;

extern {
    type Error = String;
}

Id : String = <id:r"[a-zA-Z0-9]*"> => T::schema(id);

Tuple : String = <(<Id> ",")>;

Option : Option<String> = "opt:" <Tuple?>;

Star : Vec<String> = "star:" <Tuple*>;

pub Plus : Vec<String> = "plus:" <Tuple+>;