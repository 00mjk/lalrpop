use util::tok::Tok;

grammar;

extern token {
    type Location = usize;

    enum Tok {
        "-" => Tok::Minus(..),
        "+" => Tok::Plus(..)
    }
}

pub Items: Vec<(usize, usize)> = {
    <@<> <@>> => vec![(<>)];

    <v:Items> <e:Spanned<"+">> => {
        let mut v = v;
        v.push(e);
        v
    };

    <v:Items> "-" => v;
};

Spanned<T>: (usize, usize) = {
    <@<> T <@>> => (<>);
};
