grammar {
    use util::tok::Tok;

    extern token {
        enum Tok {
            "(" => LParen,
            ")" => RParen,
            "-" => Minus,
        }
    }

    pub S = E;

    E = {
        <l:E> "-" <r:T> => l-r;

        T;
    };

    T: i32 = {
        <n:"Num"> => n.as_num();
        "(" <E> ")";
    };
}
