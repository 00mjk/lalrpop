grammar Foo {
    use util::tok::Tok;

    token Tok where {
        "(" => "LParen";
        ")" => "RParen";
        "-" => "Minus";
    };

    pub S: () =
        E => ();

    E: () = {
        E "-" T => ();
        T => ();
    };

    T: () = {
        "Num" => ();
        "(" E ")" => ();
    };
}
